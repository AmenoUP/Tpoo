    "@vue/eslint-config-prettier": "^9.0.0",
    "axios": "^1.7.7",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.23.0",
    "prettier": "^3.2.5",
    "vite": "^5.4.8",
    "vue": "^3.5.12",
    "vue-cli-plugin-cordova": "~2.4.1",
    "vue-template-compiler": "^2.7.16"
